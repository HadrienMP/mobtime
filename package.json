{
    "name": "mob-time-elm",
    "version": "1.0.22",
    "description": "",
    "scripts": {
        "build": "webpack --mode production",
        "start": "node server/app.js",
        "dev": "concurrently --kill-others \"yarn dev:build-front\" \"yarn dev:server\"",
        "dev:build-front": "webpack --mode development --watch",
        "dev:server": "nodemon server/app.js",
        "test": "npx elm-test \"./src/elm/**/*Spec.elm\"",
        "elm:remove-dead-code": "npx elm-review --template jfmengels/elm-review-unused/example --fix-all",
        "postinstall": "elm-tooling install ; husky install",
        "review": "elm-review --fix-all",
        "build:doc": "parcel build doc/index.html",
        "start:doc": "parcel doc/index.html",
        "stylelint": "stylelint **/*.scss"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@parcel/transformer-elm": "^2.8.1",
        "concurrently": "^7.0.0",
        "css-loader": "^6.5.1",
        "elm-book": "^1.0.1",
        "elm-live": "^3.4.1",
        "elm-review": "^2.8.2",
        "elm-test": "^0.19.1-revision6",
        "elm-tooling": "^1.8.0",
        "elm-webpack-loader": "^8.0.0",
        "gitmoji-cli": "^5.0.2",
        "howler": "^2.2.3",
        "https-local": "^1.0.0",
        "husky": "^8.0.2",
        "nodemon": "^2.0.7",
        "parcel": "^2.8.1",
        "prettier": "^2.8.2",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "socket.io-client": "^4.3.2",
        "style-loader": "^3.3.1",
        "stylelint": "^14.16.0",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "stylelint-config-standard": "^29.0.0",
        "stylelint-scss": "^4.3.0",
        "tippy.js": "^6.3.7",
        "webpack": "^5.64.0",
        "webpack-cli": "^4.9.1"
    },
    "release-it": {
        "npm": {
            "publish": false
        },
        "github": {
            "release": true,
            "assets": [
                "./dist/dist.zip"
            ]
        }
    },
    "source": "doc/index.html",
    "dependencies": {
        "elm": "^0.19.1-5",
        "express": "^4.17.1",
        "socket.io": "^3.1.2"
    }
}
