        "postinstall": "elm-tooling install ; husky install",
        "review": "elm-review",
        "build:doc": "parcel build doc/index.html --dist-dir=doc/dist",
        "start:doc": "parcel doc/index.html --dist-dir=doc/dist --port=4321",
